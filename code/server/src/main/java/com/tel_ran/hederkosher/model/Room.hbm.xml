<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tel_ran.hederkosher.model.Room" table="room" schema="heder-kosher">
        <id name="idRoom" column="idRoom"/>
        <property name="name" column="name"/>
        <property name="adress" column="adress"/>
        <property name="dcreate" column="dcreate"/>
        <property name="dclose" column="dclose"/>
    </class>
</hibernate-mapping>