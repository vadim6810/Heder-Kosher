<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tel_ran.hederkosher.model.Person" table="person" schema="heder-kosher">
        <id name="idPerson" column="idPerson"/>
        <property name="passportNo" column="passportNo"/>
        <property name="fistName" column="fistName"/>
        <property name="lastname" column="lastname"/>
        <property name="secondname" column="secondname"/>
        <property name="birthday" column="birthday"/>
    </class>
</hibernate-mapping>